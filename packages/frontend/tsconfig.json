  "ts-node": {
    "require": ["tsconfig-paths/register"]
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2020",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@/router/*": ["router/*"],
      "@/components/*": ["components/*"],
      "@/constants/*": ["constants/*"],
      "@/pages/*": ["pages/*"],
      "@/types/*": ["types/*"],
      "@/icons/*": ["icons/*"],
    }
  }
}
